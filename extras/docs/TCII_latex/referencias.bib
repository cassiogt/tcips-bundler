	number =	3954,
	howpublished =	{RFC 3954},
	publisher =	{RFC Editor},
	doi =		{10.17487/RFC3954},
	url =		{https://rfc-editor.org/rfc/rfc3954.txt},
        author =	{Benoit Claise},
	title =		{{Cisco Systems NetFlow Services Export Version 9}},
	pagetotal =	33,
	year =		2004,
	month =		oct,
	abstract =	{This document specifies the data export format for version 9 of Cisco Systems&#39; NetFlow services, for use by implementations on the network elements and/or matching collector programs. The version 9 export format uses templates to provide access to observations of IP packet flows in a flexible and extensible manner. A template defines a collection of fields, with corresponding descriptions of structure and semantics. This memo provides information for the Internet community.},
}



@misc{rfc3176,
	series =	{Request for Comments},
	number =	3176,
	howpublished =	{RFC 3176},
	publisher =	{RFC Editor},
	doi =		{10.17487/RFC3176},
	url =		{https://rfc-editor.org/rfc/rfc3176.txt},
        author =	{Sonia Panchen and Neil McKee and Peter Phaal},
	title =		{{InMon Corporation's sFlow: A Method for Monitoring Traffic in Switched and Routed Networks}},
	pagetotal =	31,
	year =		2001,
	month =		sep,
	abstract =	{This memo defines InMon Corporation&#39;s sFlow system. sFlow is a technology for monitoring traffic in data networks containing switches and routers. In particular, it defines the sampling mechanisms implemented in an sFlow Agent for monitoring traffic, the sFlow MIB for controlling the sFlow Agent, and the format of sample data used by the sFlow Agent when forwarding data to a central data collector. This memo provides information for the Internet community.},
}

@misc{rfc5153,
	series =	{Request for Comments},
	number =	5153,
